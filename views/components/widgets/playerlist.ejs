
<style>
#myInput {
  background-image: url('/css/searchicon.png'); /* Add a search icon to input */
  background-position: 10px 12px; /* Position the search icon */
  background-repeat: no-repeat; /* Do not repeat the icon image */
  width: 100%; /* Full-width */
  font-size: 0.9rem; /* Increase font-size */
  padding: 12px 20px 12px 40px; /* Add some padding */
  border: 1px solid #ddd; /* Add a grey border */
  margin-bottom: 12px; /* Add some space below the input */
}

#myUL {
  /* Remove default list styling */
  list-style-type: none;
  padding: 0;
  margin: 0;
}

#myUL li a {
  border: 1px solid #ddd; /* Add a border to all links */
  margin-top: -1px; /* Prevent double borders */
  background-color: #f6f6f6; /* Grey background color */
  padding: 12px; /* Add some padding */
  text-decoration: none; /* Remove default text underline */
  font-size: 0.9rem; /* Increase the font-size */
  color: black; /* Add a black text color */
  display: block; /* Make it into a block element to fill the whole list */
}

#myUL li a:hover:not(.header) {
  background-color: #eee; /* Add a hover effect to all links, except for headers */
}
</style>
<div >
    <div>
      <div class="p-4">
          <nav id="nav-menu-container">
              <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names..">
              <ul class="list-group" style="color:#fff;list-style: none;" id="myUL">
                  <% var x=0; %>
                  <% players.forEach((link)=>{%>
 
                <li class="list-group-item">
                 
                    <% if(wdate!=undefined) { %>
                      <a class="nav-link" style="text-transform: uppercase;color:black;" href="/<%= link.name %>"><span class="sr-only">(current)</span>
                       <table>
                         <tr>
                           <td><div style="width:100px;height:100px;border-radius:50px;overflow:hidden;background-color: #ccc;"><img src="/assets/imgs/members/<%=link.name %>.png" alt=""  width=100 style="border-radius:50px;"/></div></td>
                           <td style="text-align:center;vertical-align: middle;padding-left:15px;">
                        <% var l=link.name %>
                        <% var c=l.split('_').length %>
                        <% for(var p=0;p<c;p++){%> 
                          <% l= l.replace('_',' ') %>
                        <%}%>
                        <%- l %>&nbsp;
                       <span style="text-transform: capitalize;">(<%- link.title%>)</span>
                           </td>
                         </tr>
                       </table>
                     
                      
                    </a>  
                      <%}else{%>
                        <% var link1=link.name+"client" %>
                        <a class="nav-link " style="text-transform: uppercase;color:black;"  href="/<%= link1 %>"><span class="sr-only">(current)</span>
                          <table>
                            <tr>
                              <td><div style="width:100px;height:100px;border-radius:50px;overflow:hidden;background-color:#ccc;"><img src="/assets/imgs/members/<%=link.name %>.png" alt=""  width=100 style="border-radius:50px;"/></div> </td>
                              <td style="text-align:center;vertical-align: middle;padding-left:15px;">
                                <% var l=link.name %>
                                <% var c=l.split('_').length %>
                                <% for(var p=0;p<c;p++){%> 
                                  <% l= l.replace('_',' ') %>
                                <%}%>
                                <%- l %>&nbsp;
                               <span style="text-transform: capitalize;">(<%- link.title %>)</div>
                                <span class="badge pull-right" style="margin-right:20px;"></span>
                              </td>
                            </tr>
                          </table>
                         
                         
                        </a>
                        
                        <%}%>
                </li>
             
        
                <% x++; %>
                <%});%>
              </ul>
          
            </nav>
      
      </div>
    </div>
    <script>
        function myFunction() {
          // Declare variables
          var input, filter, ul, li, a, i, txtValue;
          input = document.getElementById('myInput');
          filter = input.value.toUpperCase();
          ul = document.getElementById("myUL");
          li = ul.getElementsByTagName('li');
        
          // Loop through all list items, and hide those who don't match the search query
          for (i = 0; i < li.length; i++) {
            a = li[i].getElementsByTagName("a")[0];
            txtValue = a.textContent || a.innerText;
            if (txtValue.toUpperCase().indexOf(filter) > -1) {
              li[i].style.display = "";
            } else {
              li[i].style.display = "none";
            }
          }
        }
        </script>

  </div>
